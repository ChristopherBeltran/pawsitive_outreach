<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<%= f.label :email %>
<%= f.email_field :email %>
<br>
<%= f.label :phone_number %>
<%= f.telephone_field :phone_number %>
<br>
<%= f.label :address %>
<%= f.text_area :address %>
<br>
<%= f.submit  "Create User" %>
<%end%>
